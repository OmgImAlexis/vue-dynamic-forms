{
  "name": "@asigloo/vue-dynamic-forms",
  "version": "0.0.0-development",
  "description": "Easy way to dynamically create reactive forms in vue based on varying business object model",
  "author": "Alvaro Saburido <alvaro.saburido@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:demo": "vue-cli-service build",
    "build": "rollup -c rollup.config.js",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:dts": "api-extractor run --local --verbose && tail -n +7 >> dist/as-dynamic-forms.d.ts",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "semantic-release": "semantic-release",
    "test": "vue-cli-service test:unit",
    "test:components": "vue-cli-service test:components",
    "test:watch": "vue-cli-service test:unit --verbose --no-cache --watchAll"
  },
  "main": "dist/as-dynamic-forms.cjs.js",
  "module": "dist/as-dynamic-forms.esm-bundler.js",
  "browser": "dist/as-dynamic-forms.esm.js",
  "unpkg": "dist/as-dynamic-forms.global.js",
  "dependencies": {
    "deep-clone": "^3.0.3",
    "deep-object-diff": "^1.1.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.16.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "chalk": "^4.1.1",
    "cypress": "^7.5.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.11.1",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "node-sass": "6.0.0",
    "pinst": "^2.1.6",
    "postcss": "^8.3.3",
    "prettier": "2.3.1",
    "rollup": "^2.51.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "12.1.0",
    "semantic-release": "17.4.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4",
    "tailwindcss-animatecss": "^1.0.7",
    "ts-node": "^10.0.0",
    "typescript": "~4.3.2",
    "vue": "^3.0.0",
    "vue-cli-plugin-cypress-experimental": "~1.2.0",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-router": "^4.0.1"
  },
  "jsdelivr": "dist/as-dynamic-forms.global.js",
  "types": "dist/as-dynamic-forms.d.ts"
}
